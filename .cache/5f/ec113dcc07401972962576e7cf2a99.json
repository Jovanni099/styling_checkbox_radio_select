display: block;\n            // display: inline-block;\n            // position: relative;\n            // top: 2px;\n            width: 16px;\n            height: 16px;\n            border: 1px solid #ccc;\n            border-radius: 3px;\n            background: white;\n            position: absolute;\n            top: 0;\n            left: 0;\n            z-index: 1;\n            transition: background 300ms ease-out, border 300ms ease-out;\n        }\n\n        &::after {\n            content: \"\";\n            display: block;\n            width: 16px;\n            height: 16px;\n            background: url('../assets/img/check.png') no-repeat;\n            background-size: 16px 16px;\n            opacity: 0;\n            position: absolute;\n            top: 0;\n            left: 1px;\n            z-index: 2;\n            transition: opacity 300ms ease-out;\n        }\n    }\n\n    .checkbox__input:checked+.checkbox_label::before {\n        // opacity: 1;\n        background-color: #24b31d;\n        border-color: #e6f602;\n    }\n\n    .checkbox__input:checked+.checkbox_label::after {\n        opacity: 1;\n    }\n}\n\n/* Radio */\n.radio {\n    position: relative;\n    margin-bottom: 20px;\n\n    .radio__input {\n        // appearance: none;\n        // -webkit-appearance: none;\n        position: absolute;\n    }\n\n    .radio__label {\n        font-size: 16px;\n        color: rgb(49, 24, 24);\n        padding-left: 25px;\n        cursor: pointer;\n\n        &::before {\n            content: \"\";\n            width: 16px;\n            height: 16px;\n            display: block;\n            border: 1px solid #ccc;\n            border-radius: 3px;\n            background: white;\n            position: absolute;\n            // top: 0;\n            // left: 0;\n            // z-index: 1;\n            transition: background 300ms ease-out, border 300ms ease-out;\n            background: #24b31d;\n        }\n\n        &::after {\n            content: '';\n            width: 16px;\n            height: 16px;\n            background: url('../assets/img/check.png') no-repeat;\n            background-size: 12px 12px;\n            display: block;\n            // background: #24b31d;\n            // border-radius: 50%;\n            position: absolute;\n            top: 2px;\n            left: 3px;\n            // z-index: 2;\n            opacity: 0;\n            transition: opacity 700ms ease-out;\n        }\n    }\n\n    .radio__input:checked+.radio__label::before {\n        filter: drop-shadow(1px 1px 7px rgb(8, 225, 26));\n        border-color: #0a7b04;\n    }\n\n    .radio__input:checked+.radio__label::after {\n        opacity: 1;\n    }\n}\n\n/*Select*/\n.select {\n    // appearance: none;\n    position: relative;\n    margin-bottom: 15px;\n\n    &::after {\n        content: '';\n        display: block;\n        border-style: solid;\n        border-width: 8px 7.5px 0 7.5px;\n        border-color: #84a429 transparent transparent transparent;\n        position: absolute;\n        top: 50%;\n        right: 1rem;\n        z-index: 1;\n        margin-top: -3px;\n        pointer-events: none;\n    }\n\n    select {\n        display: block;\n        width: 100%;\n        padding: .75rem 2rem .75rem 1rem;\n        background: none;\n        border: 1px solid #ccc;\n        border-radius: 3px;\n        appearance: none;\n        font-family: inherit;\n        font-size: 1rem;\n        color: #444;\n    }\n}\n\n.select select {\n    &:focus {\n        outline: 0;\n        border-color: rgb(234, 6, 93);\n    }\n}"},"lineCount":null}},"error":null,"hash":"bdb38b650bc8d825ce83342bfda3e5da","cacheData":{"env":{}}}